                                                            <option value="Administrator" selected>Administrator</option>
                                                            <option value="End-user">End-User</option>
                                                        </select>
                                                        <input type="hidden" id="UserToBeUpdated_id">
                                                    </div>
                                                    <div class="form-group">
                                                            <label for="message-text" class="control-label">Position:</label>
                                                            <textarea class="form-control" name="" id="UpdatePositionTextarea_id" cols="30" rows="10"></textarea>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="message-text" class="control-label">Main Organization Name:</label>
                                                            <textarea class="form-control" name="" id="UpdateMainOrgNameTextarea_id" cols="30" rows="10"></textarea>
                                                        </div>
                                                        <div class="form-group">
