                                                            <textarea class="form-control" name="" id="UpdateSubOrgNameTextarea_id" cols="30" rows="10"></textarea>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="message-text" class="control-label">Ext Organization Name:</label>
                                                            <textarea class="form-control" name="" id="UpdateExtOrgNameTextarea_id" cols="30" rows="10"></textarea>
