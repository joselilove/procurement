                                                            <div class="form-group">
                                                                <label class="control-label">Name</label>
                                                                <input type="text" id="UAsettingNameInput_id" class="form-control" disabled>
                                                                <label class="control-label">Email</label>
                                                                <input type="text" id="UAsettingEmailInput_id" class="form-control" disabled>
                                                            </div>
                                                    </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Username</label>
                                                        <input type="text" id="UAsettingUsernameInput_id" class="form-control" disabled>
                                                    </div>
                                                </div>
                                                
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label">Password</label>
                                                        
                                                        <div class="input-group mb-3">
                                                                <input type="password" class="form-control" placeholder="" aria-label="" aria-describedby="basic-addon1" id ="UAsettingPasswordInput_id" disabled>
                                                                <div class="input-group-append">
                                                                    <button  class="btn btn-warning" type="button" onmousedown="showPassword_FNCTN()" onmouseup="hidePassword_FNCTN()"><i class="mdi mdi-eye-outline" style="font-size:20px"></i></button>
                                                                </div>
                        
                                                            </div>
                                                    </div>
                                                </div>
                                                
                                                
                                                  
                                                       
                                                <div class="col-md-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Employee Position</label>
                                                            <textarea name="" id="UAsettingPositionTextarea_id" cols="30" rows="2" class="form-control" placeholder="Ex. BAC, Head Secretariat" disabled></textarea>
                                                        </div>
                                                </div>
                                                <div class="col-md-12">
                                                        <div class="form-group">
                                                            <label class="control-label">University Program Belonged</label>
                                                            <textarea name="" id="UAsettingProgramBelongedTextarea_id" cols="30" rows="5" class="form-control" placeholder="Ex. GENERAL ADMINISTRATION SERVICE" disabled></textarea>
                                                        </div>
                                                </div><div class="col-md-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Department Name</label>
                                                            <textarea name="" id="UAsettingDeptNameTextarea_id" cols="30" rows="5" class="form-control" placeholder="Ex. Bids And Awards Committree (BAC)" disabled></textarea>
                                                        </div>
                                                </div><div class="col-md-12">
                                                        <div class="form-group">
                                                            <label class="control-label">Sub Extention Office Name</label>
